; DIVISAO

LOAD R0 A
LOAD R1 B
COPY R7 R0
COPY R8 R0
COPY R10 R0

READ R5
READ R6

ADD R5 R0
JNN POS_1
NEG R5
COPY R7 R1
POS_1:
ADD R6 R0
JNN POS_2
NEG R6
COPY R8 R1

POS_2: 
CALL DIV
CALL SINAIS

WRITE R10
WRITE R5

HALT

DIV:
ADD R10 R1
SUB R5 R6
JZ FIMD
JN RESTO
JMP DIV
FIMD: RET

RESTO:
ADD R5 R6
SUB R10 R1
JMP FIMD

SINAIS: 
SUB R7 R1
JNZ OP2
NEG R10
NEG R5
OP2: SUB R8 R1
JNZ FIMS
NEG R10
NEG R5
FIMS: RET

A: WORD 0
B: WORD 1
